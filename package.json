{
   "name": "node-jsdom-starter",
   "description": "Sample dna-engine application running on Node.js with jsdom",
   "version": "1.0.0",
   "license": "MIT",
   "type": "module",
   "module": "app.js",
   "repository": "github:dna-engine/node-jsdom-starter",
   "homepage": "https://github.com/dna-engine/node-jsdom-starter",
   "bugs": "https://github.com/dna-engine/node-jsdom-starter/issues",
   "docs": "https://dna-engine.org/docs",
   "author": "Center Key (https://centerkey.com)",
   "keywords": [
      "node",
      "dna-engine",
      "jsdom",
      "javascript"
   ],
   "jshintConfig": {
      "esversion": 11,
      "strict": "implied",
      "eqeqeq": true,
      "undef": true,
      "unused": true,
      "node": true
   },
   "scripts": {
      "test": "jshint . --exclude-path .gitignore",
      "start": "npm test && node app"
   },
   "dependencies": {
      "dna-engine": "~3.3",
      "jsdom": "~27.0",
      "jshint": "~2.13"
   }
}
